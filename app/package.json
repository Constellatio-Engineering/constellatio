{
  "name": "@constellatio/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "separatorDev": "---------------------- DEVELOPMENT ----------------------",
    "dev-without-migrations": "next dev -p 3010",
    "dev": "node -r dotenv-flow/config ci/apply-db-migrations.mjs && pnpm dev-without-migrations",
    "sepatatorLint": "---------------------- LINT ----------------------",
    "check-types": "tsc --noEmit",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "eslint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "lint": "pnpm check-types && pnpm eslint",
    "separatorBuild": "---------------------- BUILD ----------------------",
    "build": "next build",
    "separatorStart": "---------------------- START ----------------------",
    "start": "next start -p 3010",
    "start:prod": "pnpm build && pnpm start",
    "separatorDrizzle": "---------------------- DRIZZLE ----------------------",
    "db:push": "drizzle-kit push:pg",
    "drizzle:studio": "drizzle-kit studio",
    "generate-migrations-without-row-level-security": "drizzle-kit generate:pg",
    "generate-new-custom-migration": "drizzle-kit generate:pg --custom",
    "generate-migrations": "sh ./scripts/generate-migrations.sh",
    "drop-migration": "drizzle-kit drop",
    "separatorGenerate": "---------------------- GENERATE ----------------------",
    "gen": "graphql-codegen -r dotenv/config --config codegen.ts",
    "gen:watch": "graphql-codegen --watch -r dotenv/config --config codegen.ts",
    "supabase:typegen": "npx supabase gen types typescript --project-id kzrdtppwdumjscxldnwo --schema public > src/lib/database.types.ts",
    "separatorStorybook": "---------------------- Storybook ----------------------",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "separatorGoogleCloud": "---------------------- GOOGLE CLOUD ----------------------",
    "copyServiceAccountBase64ToClipboard": "cat google/service-accounts/user-uploads-service-account.json | base64 | pbcopy && pnpm printCopiedToClipboard",
    "setGoogleCloudProject": "gcloud config set project idyllic-psyche-400317",
    "setCloudStorageDevelopmentCors": "gsutil cors set google/cloud-storage-cors/development.cors-config.json gs://constellatio-user-uploads-development",
    "setCloudStorageStagingCors": "gsutil cors set google/cloud-storage-cors/staging.cors-config.json gs://constellatio-user-uploads-staging",
    "setCloudStorageProductionCors": "gsutil cors set google/cloud-storage-cors/production.cors-config.json gs://constellatio-user-uploads-production",
    "separatorOther": "---------------------- OTHER ----------------------",
    "printCopiedToClipboard": "echo 'Copied to clipboard'",
    "localtunnel": "lt --subdomain constelaltio-app-development --port 3010"
  },

{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    /** Emit types for internal packages to speed up editor performance. */
    "declaration": true,
    "declarationMap": true,
    "sourceMap": false,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "noEmitOnError": false,
    "outDir": "${configDir}/dist",
    "skipLibCheck": true
  }
}

  "version": "0.1.0",
  "private": true,
  "scripts": {
    "separatorDev": "---------------------- NEXT.JS ----------------------",
    "next:dev": "pnpm with-env next dev --turbo -p 3010",
    "start": "pnpm with-env next start -p 3010",
    "build": "pnpm with-env next build",
    "sepatatorLint": "---------------------- LINT ----------------------",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "separatorTunnel": "---------------------- TUNNEL ----------------------",
    "ngrok-auth": "ngrok authtoken $NGROK_AUTHTOKEN",
    "tunnel": "ngrok start --config=ngrok.yml constellatio-web-app",
    "separatorGoogleCloud": "---------------------- GOOGLE CLOUD ----------------------",
    "copyServiceAccountBase64ToClipboard": "cat google/service-accounts/user-uploads-service-account.json | base64 | pbcopy && pnpm printCopiedToClipboard",
    "setGoogleCloudProject": "gcloud config set project idyllic-psyche-400317",
    "setCloudStorageDevelopmentCors": "gsutil cors set google/cloud-storage-cors/development.cors-config.json gs://constellatio-user-uploads-development",
    "setPublicCloudStorageDevelopmentCors": "gsutil cors set google/cloud-storage-cors/development.cors-config.json gs://constellatio-public-user-uploads-development",
    "setCloudStorageStagingCors": "gsutil cors set google/cloud-storage-cors/staging.cors-config.json gs://constellatio-user-uploads-staging",
    "setPublicCloudStorageStagingCors": "gsutil cors set google/cloud-storage-cors/staging.cors-config.json gs://constellatio-public-user-uploads-staging",
    "setCloudStorageProductionCors": "gsutil cors set google/cloud-storage-cors/production.cors-config.json gs://constellatio-user-uploads-production",
    "setPublicCloudStorageProductionCors": "gsutil cors set google/cloud-storage-cors/production.cors-config.json gs://constellatio-public-user-uploads-production",
    "separatorShadcn": "---------------------- SHADCN ----------------------",
    "ui-add": "pnpm dlx shadcn@latest add && prettier src --write --list-different",
    "updateAllInstalledComponents": "comingSoon...",
    "separatorOther": "---------------------- OTHER ----------------------",
    "printCopiedToClipboard": "echo 'Copied to clipboard'",
    "with-env": "dotenv-run -- "
  },

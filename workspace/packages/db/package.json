{
  "name": "@constellatio/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./src/schema.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "apply-migrations": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "generate-migrations-without-row-level-security": "drizzle-kit generate",
    "generate-new-custom-migration": "drizzle-kit generate --custom",
    "generate-migrations": "sh ./scripts/generate-migrations.sh",
    "drop-migration": "drizzle-kit drop",
    "check-migrations": "drizzle-kit check"
  },
  "dependencies": {
    "@constellatio/db-to-search": "workspace:*",
    "@constellatio/env": "workspace:*",
    "@constellatio/schemas": "workspace:*",
    "@constellatio/shared": "workspace:*",
    "@constellatio/utils": "workspace:*",
    "@t3-oss/env-nextjs": "catalog:",
    "drizzle-orm": "0.35.3",
    "postgres": "catalog:"
  },
  "devDependencies": {
    "@constellatio/eslint-config": "workspace:*",
    "@constellatio/tsconfig": "workspace:*",
    "drizzle-kit": "0.26.2",
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}

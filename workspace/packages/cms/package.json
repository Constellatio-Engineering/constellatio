{
  "name": "@constellatio/cms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./sdk": {
      "types": "./dist/graphql/getSdk.d.ts",
      "default": "./src/graphql/getSdk.ts"
    },
    "./generated-types": {
      "types": "./dist/graphql/__generated/sdk.d.ts",
      "default": "./src/graphql/__generated/sdk.ts"
    },
    "./content/*": {
      "types": "./dist/content/*.d.ts",
      "default": "./src/content/*.ts"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "default": "./src/utils/*.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@constellatio/env": "workspace:*",
    "@constellatio/shared": "workspace:*",
    "@constellatio/utility-types": "workspace:*",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "superjson": "catalog:"
  },
  "devDependencies": {
    "@constellatio/eslint-config": "workspace:*",
    "@constellatio/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}

{
  "name": "@constellatio/schemas",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./schemas/*": {
      "types": "./dist/schemas/*.d.ts",
      "default": "./src/schemas/*.ts"
    },
    "./common/*": {
      "types": "./dist/common/*.d.ts",
      "default": "./src/common/*.ts"
    }
  },
  "typesVersions": {
    "*": {
      "schemas/*": ["./dist/schemas/*"],
      "common/*": ["./dist/common/*"]
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@constellatio/env": "workspace:*",
    "@constellatio/utils": "workspace:*",
    "@constellatio/shared": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@constellatio/tsconfig": "workspace:*",
    "@constellatio/eslint-config": "workspace:*",
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}
